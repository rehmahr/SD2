<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FED98E"
    tools:context=".ProgressReport">

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView18"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:cardBackgroundColor="#00FFFFFF"
        app:cardElevation="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <com.google.android.material.navigation.NavigationView
            android:id="@+id/Menubar"
            android:layout_width="90dp"
            android:layout_height="458dp"
            android:background="@drawable/rounded_corner_navigation_view"
            android:elevation="60dp"
            android:visibility="visible"
            app:itemIconTint="@color/Grey"

            tools:visibility="visible">


            <ImageButton
                android:id="@+id/home_button"
                android:layout_width="55dp"
                android:layout_height="59dp"
                android:background="@drawable/more_round"
                android:scaleType="fitCenter"
                android:src="@drawable/home_button"
                android:translationX="15dp"
                android:translationY="50dp"
                android:visibility="visible" />

            <ImageButton
                android:id="@+id/report"
                android:layout_width="55dp"
                android:layout_height="59dp"
                android:background="@drawable/more_round"
                android:scaleType="fitCenter"
                android:src="@drawable/report"
                android:translationX="15dp"
                android:translationY="150dp" />

            <ImageButton
                android:id="@+id/feedback"
                android:layout_width="55dp"
                android:layout_height="59dp"
                android:background="@drawable/more_round"
                android:scaleType="fitCenter"
                android:src="@drawable/icons8_feedback_100"
                android:translationX="15dp"
                android:translationY="250dp" />

            <ImageButton
                android:id="@+id/help"
                android:layout_width="55dp"
                android:layout_height="59dp"
                android:background="@drawable/more_round"
                android:scaleType="fitCenter"
                android:src="@drawable/help"
                android:translationX="15dp"
                android:translationY="350dp" />

        </com.google.android.material.navigation.NavigationView>
    </androidx.cardview.widget.CardView>

    <ScrollView
        android:layout_width="1188dp"
        android:layout_height="898dp"
        android:layout_marginStart="1dp"
        android:layout_marginTop="1dp"
        android:layout_marginEnd="1dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/cardView18"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" >

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="1150dp">

                <androidx.cardview.widget.CardView
                    android:id="@+id/cardView20"
                    android:layout_width="1000dp"
                    android:layout_height="250dp"
                    android:layout_marginStart="94dp"
                    android:layout_marginTop="94dp"
                    android:layout_marginEnd="94dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent">

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <androidx.cardview.widget.CardView
                            android:id="@+id/cardView22"
                            android:layout_width="200dp"
                            android:layout_height="180dp"
                            android:layout_marginStart="40dp"
                            android:layout_marginTop="35dp"
                            android:layout_marginBottom="35dp"
                            app:cardBackgroundColor="#87CFD8"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent">

                        </androidx.cardview.widget.CardView>

                        <androidx.cardview.widget.CardView
                            android:layout_width="200dp"
                            android:layout_height="180dp"
                            android:layout_marginStart="41dp"
                            android:layout_marginTop="35dp"
                            android:layout_marginBottom="35dp"
                            app:cardBackgroundColor="#87CFD8"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintStart_toEndOf="@+id/cardView22"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintVertical_bias="0.0">

                        </androidx.cardview.widget.CardView>

                        <androidx.cardview.widget.CardView
                            android:id="@+id/cardview"
                            android:layout_width="200dp"
                            android:layout_height="180dp"
                            android:layout_marginTop="35dp"
                            android:layout_marginEnd="40dp"
                            android:layout_marginBottom="35dp"
                            app:cardBackgroundColor="#87CFD8"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toStartOf="@+id/cardView23"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintVertical_bias="0.0">

                        </androidx.cardview.widget.CardView>

                        <androidx.cardview.widget.CardView
                            android:id="@+id/cardView23"
                            android:layout_width="200dp"
                            android:layout_height="180dp"
                            android:layout_marginTop="35dp"
                            android:layout_marginEnd="40dp"
                            android:layout_marginBottom="35dp"
                            app:cardBackgroundColor="#87CFD8"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintVertical_bias="0.0" />
                    </androidx.constraintlayout.widget.ConstraintLayout>

                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:id="@+id/cardView21"
                    android:layout_width="1000dp"
                    android:layout_height="250dp"
                    android:layout_marginStart="94dp"
                    android:layout_marginTop="94dp"
                    android:layout_marginEnd="94dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/cardView20">

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <androidx.cardview.widget.CardView
                            android:layout_width="200dp"
                            android:layout_height="180dp"
                            android:layout_marginStart="40dp"
                            android:layout_marginTop="35dp"
                            android:layout_marginBottom="35dp"
                            app:cardBackgroundColor="#D490DF"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintStart_toEndOf="@+id/cardView25"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintVertical_bias="0.0" />

                        <androidx.cardview.widget.CardView
                            android:id="@+id/cardView25"
                            android:layout_width="200dp"
                            android:layout_height="180dp"
                            android:layout_marginStart="41dp"
                            android:layout_marginTop="35dp"
                            android:layout_marginBottom="35dp"
                            app:cardBackgroundColor="#D490DF"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintVertical_bias="1.0">

                        </androidx.cardview.widget.CardView>

                        <androidx.cardview.widget.CardView
                            android:layout_width="200dp"
                            android:layout_height="180dp"
                            android:layout_marginTop="35dp"
                            android:layout_marginEnd="40dp"
                            android:layout_marginBottom="35dp"
                            app:cardBackgroundColor="#D490DF"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toStartOf="@+id/cardView24"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintVertical_bias="1.0">

                        </androidx.cardview.widget.CardView>

                        <androidx.cardview.widget.CardView
                            android:id="@+id/cardView24"
                            android:layout_width="200dp"
                            android:layout_height="180dp"
                            android:layout_marginTop="35dp"
                            android:layout_marginEnd="40dp"
                            android:layout_marginBottom="35dp"
                            app:cardBackgroundColor="#D490DF"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintVertical_bias="1.0">

                        </androidx.cardview.widget.CardView>

                    </androidx.constraintlayout.widget.ConstraintLayout>
                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:layout_width="1000dp"
                    android:layout_height="250dp"
                    android:layout_marginStart="94dp"
                    android:layout_marginTop="94dp"
                    android:layout_marginEnd="94dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/cardView21" >

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <androidx.cardview.widget.CardView
                            android:id="@+id/cardView27"
                            android:layout_width="200dp"
                            android:layout_height="186dp"
                            android:layout_marginStart="40dp"
                            android:layout_marginTop="35dp"
                            android:layout_marginBottom="35dp"
                            app:cardBackgroundColor="#FE9DB0"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintVertical_bias="0.333">

                        </androidx.cardview.widget.CardView>

                        <androidx.cardview.widget.CardView
                            android:layout_width="200dp"
                            android:layout_height="186dp"
                            android:layout_marginStart="40dp"
                            android:layout_marginTop="35dp"
                            android:layout_marginBottom="35dp"
                            app:cardBackgroundColor="#FE9DB0"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintStart_toEndOf="@+id/cardView27"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintVertical_bias="0.0">

                        </androidx.cardview.widget.CardView>

                        <androidx.cardview.widget.CardView
                            android:layout_width="200dp"
                            android:layout_height="186dp"
                            android:layout_marginTop="35dp"
                            android:layout_marginEnd="40dp"
                            android:layout_marginBottom="35dp"
                            app:cardBackgroundColor="#FE9DB0"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toStartOf="@+id/cardView26"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintVertical_bias="0.0">

                        </androidx.cardview.widget.CardView>

                        <androidx.cardview.widget.CardView
                            android:id="@+id/cardView26"
                            android:layout_width="200dp"
                            android:layout_height="186dp"
                            android:layout_marginTop="35dp"
                            android:layout_marginEnd="40dp"
                            android:layout_marginBottom="35dp"
                            app:cardBackgroundColor="#FE9DB0"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintVertical_bias="0.0" />
                    </androidx.constraintlayout.widget.ConstraintLayout>
                </androidx.cardview.widget.CardView>
            </androidx.constraintlayout.widget.ConstraintLayout>
        </LinearLayout>
    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>