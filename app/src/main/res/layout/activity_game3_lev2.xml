<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FAA4BD"
    tools:context=".Game3Lev2"
    tools:layout_editor_absoluteX="-322dp"
    tools:layout_editor_absoluteY="-195dp">

    <com.google.android.material.navigation.NavigationView
        android:id="@+id/Menubar"
        android:layout_width="90dp"
        android:layout_height="458dp"
        android:layout_marginTop="209dp"
        android:layout_marginBottom="233dp"
        android:background="@drawable/rounded_corner_navigation_view"
        android:elevation="60dp"
        android:visibility="visible"
        app:itemIconTint="@color/Grey"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"

        app:layout_constraintTop_toTopOf="parent"
        tools:visibility="visible">


        <ImageButton
            android:id="@+id/home_button"
            android:layout_width="55dp"
            android:layout_height="59dp"
            android:background="@drawable/more_round"
            android:scaleType="fitCenter"
            android:src="@drawable/home_button"
            android:translationX="15dp"
            android:translationY="50dp"
            android:visibility="visible" />

        <ImageButton
            android:id="@+id/report"
            android:layout_width="55dp"
            android:layout_height="59dp"
            android:background="@drawable/more_round"
            android:scaleType="fitCenter"
            android:src="@drawable/report"
            android:translationX="15dp"
            android:translationY="150dp" />

        <ImageButton
            android:id="@+id/feedback"
            android:layout_width="55dp"
            android:layout_height="59dp"
            android:background="@drawable/more_round"
            android:scaleType="fitCenter"
            android:src="@drawable/icons8_feedback_100"
            android:translationX="15dp"
            android:translationY="250dp" />

        <ImageButton
            android:id="@+id/help"
            android:layout_width="55dp"
            android:layout_height="59dp"
            android:background="@drawable/more_round"
            android:scaleType="fitCenter"
            android:src="@drawable/help"
            android:translationX="15dp"
            android:translationY="350dp" />

    </com.google.android.material.navigation.NavigationView>

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="336dp"
        android:layout_marginTop="120dp"
        android:layout_marginBottom="13dp"
        android:fontFamily="@font/bungee"
        android:shadowColor="@color/pink"
        android:shadowDx="-20"
        android:shadowDy="-2"
        android:shadowRadius="20"
        android:text="match the words with faces!"
        android:textColor="@color/purple"
        android:textSize="40sp"
        android:visibility="visible"
        app:layout_constraintBottom_toTopOf="@+id/cardView3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="120dp"
        android:layout_marginStart="253dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="251dp"
        android:layout_marginBottom="10dp"

        android:elegantTextHeight="true"
        android:fontFamily="@font/bungee_inline"
        android:outlineAmbientShadowColor="@color/beige"
        android:shadowColor="@color/pink"
        android:shadowDx="-20"
        android:shadowRadius="1"
        android:text="MEMORY MATCHING"
        android:textAlignment="center"
        android:textColor="@color/purple"
        android:textSize="70sp"
        android:typeface="monospace"
        android:visibility="visible"
        app:layout_constraintBottom_toTopOf="@+id/cardView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView3"
        android:layout_width="1038dp"
        android:layout_height="481dp"
        android:layout_marginTop="150dp"
        android:layout_marginBottom="150dp"
        app:cardBackgroundColor="#e5e5e5"
        app:cardCornerRadius="20dp"
        app:cardElevation="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/Menubar"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageButton
                android:id="@+id/front_side1"
                android:layout_width="145dp"
                android:layout_height="130dp"
                android:layout_marginStart="129dp"
                android:layout_marginTop="71dp"
                android:elevation="10dp"
                android:scaleType="fitCenter"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/front_side" />

            <ImageButton
                android:id="@+id/front_side2"
                android:layout_width="145dp"
                android:layout_height="130dp"
                android:layout_marginStart="51dp"
                android:layout_marginTop="71dp"
                android:elevation="10dp"
                android:scaleType="fitCenter"
                app:layout_constraintStart_toEndOf="@+id/front_side1"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/front_side" />

            <ImageButton
                android:id="@+id/front_side3"
                android:layout_width="145dp"
                android:layout_height="130dp"
                android:layout_marginStart="56dp"
                android:layout_marginTop="71dp"
                android:elevation="10dp"
                android:scaleType="fitCenter"
                app:layout_constraintStart_toEndOf="@+id/front_side2"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/front_side" />

            <ImageButton
                android:id="@+id/front_side4"
                android:layout_width="145dp"
                android:layout_height="130dp"
                android:layout_marginTop="71dp"
                android:layout_marginEnd="144dp"
                android:elevation="20dp"
                android:scaleType="fitCenter"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/front_side" />

            <ImageButton
                android:id="@+id/front_side5"
                android:layout_width="145dp"
                android:layout_height="130dp"
                android:layout_marginStart="129dp"
                android:layout_marginBottom="71dp"
                android:elevation="20dp"
                android:scaleType="fitCenter"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:srcCompat="@drawable/front_side" />

            <ImageButton
                android:id="@+id/front_side6"
                android:layout_width="145dp"
                android:layout_height="130dp"
                android:layout_marginStart="51dp"
                android:layout_marginBottom="71dp"
                android:elevation="20dp"
                android:scaleType="fitCenter"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toEndOf="@+id/front_side5"
                app:srcCompat="@drawable/front_side" />

            <ImageButton
                android:id="@+id/front_side7"
                android:layout_width="145dp"
                android:layout_height="130dp"
                android:layout_marginStart="56dp"
                android:layout_marginBottom="71dp"
                android:elevation="20dp"
                android:scaleType="fitCenter"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toEndOf="@+id/front_side6"
                app:srcCompat="@drawable/front_side" />

            <ImageButton
                android:id="@+id/front_side8"
                android:layout_width="145dp"
                android:layout_height="130dp"
                android:layout_marginEnd="144dp"
                android:layout_marginBottom="68dp"
                android:elevation="10dp"
                android:scaleType="fitCenter"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:srcCompat="@drawable/front_side" />

        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.cardview.widget.CardView>

    <ImageView
        android:id="@+id/imageView20"
        android:layout_width="124dp"
        android:layout_height="96dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="280dp"
        android:rotation="26"
        android:rotationY="180"
        app:layout_constraintBottom_toBottomOf="@+id/cardView3"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/star" />

    <ImageView
        android:id="@+id/imageView26"
